{
  "name": "rootstock-subsquid-indexer",
  "version": "1.0.0",
  "description": "Subsquid indexer for Rootstock blockchain",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "db:create": "squid-typeorm-migration create",
    "db:migrate": "squid-typeorm-migration-apply",
    "db:generate": "squid-typeorm-migration-generate Init",
    "db:drop": "squid-typeorm-migration-revert",
    "processor:start": "node -r dotenv/config lib/main.js",
    "query:start": "npx @subsquid/graphql-server",
    "up": "docker compose up -d",
    "down": "docker compose down",
    "codegen": "squid-typeorm-codegen",
    "clean": "rm -rf lib db/migrations/*.js"
  },
  "dependencies": {
    "@subsquid/evm-processor": "^1.27.3",
    "@subsquid/typeorm-store": "^1.5.1",
    "@subsquid/graphql-server": "^4.11.0",
    "@subsquid/util-internal-hex": "^1.2.2",
    "@subsquid/archive-registry": "^3.3.2",
    "@subsquid/evm-abi": "^0.3.1",
    "@subsquid/evm-codec": "^0.3.0",
    "typeorm": "^0.3.27",
    "pg": "^8.16.3",
    "dotenv": "^16.6.1",
    "ethers": "^6.15.0"
  },
  "devDependencies": {
    "@subsquid/typeorm-codegen": "^2.0.0",
    "@subsquid/typeorm-migration": "^1.3.0",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.2"
  }
}